{
  "name": "<%= moduleName %>",
  "version": "0.0.0",
  "description": "My cool module",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sotojuan/<%= moduleName %>"
  },
  "author": {
    "name": "<%= name %>",
    "email": "<%= email %>",
    "url": "<%= humanizedWebsite %>"
  },<% if (cli) { %>
  "bin": "cli.js",<% } %>
  "engines": {
    "node": ">=4.2.0"
  },
  "scripts": {
    "test": "semistandard --verbose | snazzy && NODE_ENV=test ava"
  },
  "files": [
    "index.js"<% if (cli) { %>,
    "cli.js"<% } %>
  ],
  "keywords": [<% if (cli) { %>
    "cli-app",
    "cli",<% } %>
    ""
  ],
  "dependencies": {<% if (cli) { %>
    "meow": "^3.6.0"
  <% } %>},
  "devDependencies": {
    "ava": "*",
    "cz-conventional-changelog": "*",
    <% if (cli) { %>"execa": "*", <% } %>
    "ghooks": "*",
    "semistandard": "*",
    "snazzy": "*"
  },
  "semistandard": {
    "ignore": [
      "test.js"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "npm run test"
    }
  }
}
